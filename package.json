{
  "name": "electron-music-player",
  "version": "1.0.0",
  "description": "Lossless Audio Player",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/prebuild.js",
    "postinstall": "node scripts/postinstall.js",
    "dev": "node scripts/prebuild.js && node --max_old_space_size=2048 scripts/start.js",
    "build": "node scripts/build.js",
    "start": "electron index.js",
    "dist": "node scripts/dist.js"
  },
  "author": {
    "name": "Krisztian Korosi",
    "email": "koeroesi86@gmail.com",
    "url": "http://chris.koro.si"
  },
  "homepage": "https://github.com/Koeroesi86/electron-music-player",
  "build": {
    "appId": "com.squirrel.emusic",
    "productName": "eMusic",
    "compression": "normal",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*",
      "icons/**/*",
      "assets/**/*",
      "config/**/*",
      "migrations/**/*",
      "index.js",
      "package.json"
    ],
    "win": {
      "icon": "icons/win/icon.ico",
      "target": [ "nsis", "msi", "zip" ]
    },
    "mac": {
      "category": "public.app-category.music",
      "target": [ "dmg", "zip" ],
      "icon": "icons/mac/icon.icns"
    },
    "linux": {
      "artifactName": "eMusic-${version}.${ext}",
      "executableName": "eMusic",
      "target": [
        "AppImage", "snap", "deb", "rpm", "freebsd", "pacman", "p5p", "apk", "7z", "zip", "tar.gz"
      ],
      "icon": "icons/png/"
    },
    "publish": {
      "provider": "generic",
      "url": "http://localhost",
      "channel": "latest",
      "publishAutoUpdate": true
    }
  },
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "electron-vibrancy": "^0.1.3",
    "electron-window-state": "^4.1.0",
    "express": "^4.16.3",
    "find-free-port": "^2.0.0",
    "in-view": "^0.6.1",
    "lodash": "^4.17.10",
    "mime-types": "^2.1.20",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "music-metadata": "^2.6.1",
    "playlist-parser": "^0.0.12",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^16.4.2",
    "react-icons": "^3.0.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sharp": "^0.20.7",
    "slugify": "^1.3.1",
    "sqlite3": "^4.0.2",
    "windows-shortcuts-appid": "^0.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-icon-maker": "^0.0.4",
    "electron-packager": "^12.1.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "exports-loader": "^0.6.4",
    "file-loader": "^2.0.0",
    "imports-loader": "^0.7.1",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "react-dev-utils": "^5.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.22.1",
    "url-loader": "^0.5.8",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
