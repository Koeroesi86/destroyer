{
  "name": "electron-music-player",
  "version": "2.0.1",
  "description": "Lossless Audio Player",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -r bundle || true",
    "prebuild": "yarn run clean",
    "postinstall": "node scripts/postinstall.js",
    "dev": "node --max_old_space_size=2048 scripts/start.js",
    "build": "webpack --progress --config config/webpack.config.base.js",
    "start": "electron index.js",
    "pack:osx": "rm -rf Destroyer-darwin-x64 && NODE_ENV=production webpack --progress -p && electron-packager ./ Destroyer --platform=darwin --arch=x64 --icon=icons.icns --prune",
    "pack:win": "cross-env NODE_ENV=production webpack --progress -p && electron-packager ./ Destroyer --out=dist/windows --platform=win32 --arch=x64 --overwrite --icon=icons.ico --prune"
  },
  "author": "Krisztian Korosi",
  "build": {
    "appId": "com.squirrel.emusic"
  },
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "date-fns": "^1.28.2",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.2",
    "electron-vibrancy": "^0.1.3",
    "electron-window-state": "^4.1.0",
    "express": "^4.16.3",
    "find-free-port": "^2.0.0",
    "in-view": "^0.6.1",
    "key": "^0.1.11",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "mmmagic": "^0.5.1",
    "moment": "^2.22.2",
    "music-metadata": "^2.6.1",
    "paper": "^0.11.5",
    "playlist-parser": "^0.0.12",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^16.4.2",
    "react-icons": "^3.0.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "request": "^2.88.0",
    "scroll-animator": "^1.1.0",
    "sharp": "^0.20.7",
    "slugify": "^1.3.1",
    "sqlite3": "^4.0.2",
    "walk": "^2.3.9",
    "windows-shortcuts-appid": "^0.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^1.0.0",
    "electron-icon-maker": "^0.0.4",
    "electron-packager": "^12.1.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "exports-loader": "^0.6.4",
    "file-loader": "^2.0.0",
    "imports-loader": "^0.7.1",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "react-dev-utils": "^5.0.1",
    "sass-loader": "^7.1.0",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "style-loader": "^0.22.1",
    "url-loader": "^0.5.8",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
